        "title": "ðŸš€ DevSnip Pro: List and Remove Console Logs"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "sayaib.hue-console.createCustomSnippet",
          "group": "navigation"
        },
        {
          "command": "sayaib.hue-console.showSnippets",
          "group": "navigation",
          "when": "editorTextFocus"
        },
        {
          "command": "sayaib.hue-console.listAndRemoveConsoleLogs",
          "group": "navigation",
          "when": "editorTextFocus"
        }
      ]
    }
